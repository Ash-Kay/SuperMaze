; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[android]

modules="org/godotengine/godot/GodotAdMob,org/godotengine/godot/GodotPaymentV3"

[application]

config/name="SuperMaze"
run/main_scene="res://Scene/MainMenu.tscn"
config/icon="res://icon.png"
config/quit_on_go_back=false

[autoload]

GameManager="*res://Script/GameManager.gd"
AD="*res://Script/AD.gd"
IAP="*res://Script/IAP.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=270
window/size/test_height=480
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"
window/stretch/shrink="1"
window/handheld/emulate_touchscreen=true

[input]

ui_click={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[rendering]

2d/snapping/use_gpu_pixel_snap=true
quality/2d/use_pixel_snap=true
